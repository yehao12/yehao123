buffets = ('fried chicken', 'chicken wings', 'steak', 'beef roll', 'beef noodles')
print('\nold buffets:')
for buffet in buffets:
    print(buffet)

buffets = ('fried chicken01', 'chicken wings02', 'steak03', 'beef roll', 'beef noodles')
print('\nnew buffets:')
for buffet in buffets:
    print(buffet)
